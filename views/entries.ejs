<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/style.css">
    </head>
    <body>
        <% include menu %>
        <% if(entries.length !== 0){ %>
            <% entries.forEach((entry) =>{ %>
            <div class="entry">
                <h3><%= entry.title %></h3>
                <p><%= entry.body %></p>
                <p>Posted by <%= entry.username %></p>
            </div>
            <% }) %>
        <% }else{ %>
            <p>没有entries数据</p>
        <% } %>


    </body>
</html>